<Window x:Class="DesktopCharacter.View.Setting"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DesktopCharacter.View"
        xmlns:st="clr-namespace:DesktopCharacter.View.SettingTabView"
        xmlns:vm="clr-namespace:DesktopCharacter.ViewModel"
        mc:Ignorable="d"
        Title="Setting" Height="300" Width="300">
    <Window.DataContext>
        <vm:SettingViewModel></vm:SettingViewModel>
    </Window.DataContext>
    <Grid>
        <TabControl x:Name="tabControl" HorizontalAlignment="Left" VerticalAlignment="Top" Height="300" Width="300">
            <TabItem Header="トーク">
                <st:TalkTab></st:TalkTab>
            </TabItem>
            <TabItem Header="ランチャー">
                <st:LauncehrTab></st:LauncehrTab>
            </TabItem>
        </TabControl>
    </Grid>
</Window>
